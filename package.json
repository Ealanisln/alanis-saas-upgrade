{
  "name": "alanis-saas",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local next dev --turbopack",
    "build": "dotenv -e .env.local -- pnpm prisma:deploy && dotenv -e .env.local next build",
    "start": "dotenv -e .env.local next start",
    "lint": "next lint",
    "prisma:deploy": "dotenv -e .env.local prisma migrate deploy && dotenv -e .env.local prisma generate",
    "prisma:generate": "dotenv -e .env.local prisma generate",
    "prisma:migrate": "dotenv -e .env.local prisma migrate deploy",
    "prisma:studio": "dotenv -e .env.local prisma studio",
    "prisma:reset": "dotenv -e .env.local prisma migrate reset --force",
    "db:push": "dotenv -e .env.local prisma db push",
    "seed": "dotenv -e .env.local -- ts-node --compiler-options {\"module\":\"CommonJS\"} ./seed/seed-database.ts",
    "postinstall": "dotenv -e .env.local prisma generate"
  },
  "dependencies": {
    "@auth/core": "^0.37.4",
    "@auth/prisma-adapter": "^2.7.4",
    "@portabletext/react": "^3.2.1",
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@sanity/icons": "^3.6.0",
    "@sanity/image-url": "^1.1.0",
    "@sanity/insert-menu": "^1.1.1",
    "@sanity/logos": "^2.1.13",
    "@sanity/ui": "^2.13.1",
    "@sanity/vision": "^3.75.1",
    "@sendgrid/mail": "^8.1.4",
    "@stripe/stripe-js": "^5.6.0",
    "@vercel/analytics": "^1.5.0",
    "bcryptjs": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "8.5.2",
    "eslint-config-next": "15.1.7",
    "fs": "0.0.1-security",
    "lucide-react": "^0.475.0",
    "next": "15.1.7",
    "next-auth": "5.0.0-beta.25",
    "next-sanity": "^9.8.57",
    "next-themes": "^0.4.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-modal-video": "^2.0.2",
    "react-rx": "^4.1.19",
    "react-syntax-highlighter": "^15.6.1",
    "sanity": "^3.75.1",
    "stripe": "^17.6.0",
    "styled-components": "^6.1.15",
    "swiper": "^11.2.4",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "typewriter-effect": "^2.21.0",
    "vercel": "^41.1.4",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.13.4",
    "@types/react": "19.0.8",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "autoprefixer": "^10.4.20",
    "eslint": "9.20.1",
    "postcss": "^8.5.2",
    "prettier": "^3.5.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
